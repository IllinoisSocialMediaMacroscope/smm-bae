doctype html
html
    head
        meta(charset="utf-8")
        meta(http-equiv="X-UA-Compatible", content="IE=edge")
        meta(name="viewport", content="width=device-width, inital-scale=1")

        <!-- ajax and jquery -->
        script(src="https://ajax.googleapis.com/ajax/libs/jquery/3.2.1/jquery.min.js")
        script(src="https://code.jquery.com/ui/1.12.1/jquery-ui.js")
        script(src="https://code.jquery.com/jquery-1.12.4.js")

        <!-- bootstrap -->
        link(rel="stylesheet", href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css",
        integrity="sha384-BVYiiSIFeK1dGmJRAkycuHAHRg32OmUcww7on3RYdg4Va+PmSTsz/K68vbdEjh4u", crossorigin="anonymous")
        link(rel="stylesheet", href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap-theme.min.css",
        integrity="sha384-rHyoN1iRsVXV4nD0JutlnGaslCJuC7uwjduW9SVrLvRYooPp2bWYgmgJQIXwl/Sp", crossorigin="anonymous")
        script(src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/js/bootstrap.min.js",
        integrity="sha384-Tc5IQib027qvyjSMfHjOMaLkfuWVxZxUPnCJA7l2mCWNIpG9mGCD8wGNIcPD7Txa", crossorigin="anonymous")

        <!-- fontawesome -->
        link(rel="stylesheet", href="https://use.fontawesome.com/releases/v5.0.13/css/all.css",
        integrity="sha384-DNOHZ68U8hZfKXOrtjWvjxusGo9WQnrNx2sqG0tfsghAvtVlRW3tvkXWZh58N9jp", crossorigin="anonymous")

        <!-- google chart -->
        script(type="text/javascript", src="https://www.gstatic.com/charts/loader.js")

        <!-- google font -->
        link(href='https://fonts.googleapis.com/css?family=Monda', rel='stylesheet')
        link(href='https://fonts.googleapis.com/css?family=Kaushan Script', rel='stylesheet')
        link(href="css/topbar.css", rel="stylesheet")

        <!-- Count up -->
        script(type="text/javascript", src="javascript/countUp.js")

        <!-- tooltip -->
        script(src="javascript/popper.js")

        <!-- d3 -->
        script(type="text/javascript", src="https://d3js.org/d3.v5.min.js")

        <!-- customized -->
        link(href="css/header.css", rel="stylesheet")
        link(href="css/auth.css", rel="stylesheet")
        link(href="css/index.css", rel="stylesheet")

        body
            include topbar.pug
            include header.pug
            include preview.pug

            div(id="history")
                div(class="col col-md-4 col-sm-4 col-xs-12")
                    div(id="history-links")
                div(class="col col-md-8 col-sm-8 col-xs-12")
                    div(id="bulk-comparison")
                        div(class="personality-header")
                            h2 Bulk Comparison
                        div(id="history-algorithm")
                            p <b><i>We can only compare results generated by the same algorithms. More algorithms coming soon...</i></b>
                            label(class="radio-inline")
                                input(type="radio", name="history-algorithm", value="IBM-Personality", checked)
                                | IBM Personality
                            label(class="radio-inline")
                                input(type="radio", name="history-algorithm", value="Pamuksuz-Personality")
                                | Pamuksuz Brand Personality
                        div(id="history-form")
                        div(id="history-chart")
                        div(id="history-chart-legend")

            include warning.pug
            include error.pug

            script(src="javascript/calcSessionID.js")
            script(src="javascript/termination.js")
            script(src="javascript/formValidation.js")
            script(src="javascript/previewPublic.js")
            script(src="javascript/historyPublic.js")
